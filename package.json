{
  "name": "vue-element-query",
  "version": "3.0.1",
  "description": "VueJS mixin plugin for creating element size queries in components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "webpack serve --hot --open --env development --mode development",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "lint": "eslint src example --ext js,vue",
    "format": "npm run lint -- --fix"
  },
  "author": "Enzo Eghermanne <enzo.eghermanne@district01.be>",
  "license": "MIT",
  "repository": "github:e2o/vue-element-query",
  "keywords": [
    "vue",
    "vuejs",
    "element",
    "query",
    "queries",
    "responsive",
    "component",
    "plugin",
    "mixin",
    "ResizeObserver"
